            $html .= '&lt;p&gt;Message      : &lt;/p&gt;';
            $html .= '&lt;p&gt;'.$message.'&lt;/p&gt;';
            $html .= '&lt;/div&gt;&lt;/body&gt;&lt;/html&gt;';

            //$fn = $this-&gt;config-&gt;item('admin_email','ion_auth');
            $fn = 'binaya619@gmail.com';


            $to = $fn;
            $from = $emailid;
            $header = "From:" . $from . " \r\n";
